{
  "name": "nighten/api-client",
  "description": "Nighten API client",
  "keywords": [
    "API"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Golovanov",
      "email": "aa.golovanov@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Nighten\\ApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nighten\\ApiClient\\Tests\\": "Tests/"
    }
  }
}
